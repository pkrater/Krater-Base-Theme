<header class="banner navbar navbar-default navbar-responsive navbar-fixed-top navbar-not-home" role="banner">
  <div class="container">
    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target=".navbar-collapse">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="{{ wp.home_url() }}"><img src="./wp-content/themes/krater-base-theme/dist/img/loggasmall.png" /></a>
    </div>


    <nav class="collapse navbar-collapse navbar-right" role="navigation">
        {% if wp.has_nav_menu('primary_navigation') %}
            <div class="collapse navbar-collapse">
                {{ wp.wp_nav_menu({
                    'theme_location':'primary_navigation',
                    'menu_class':'nav navbar-nav',
                    'walker': wp.returnObject('wp_bootstrap_navwalker'),
                    'container':false,
                    'depth':2,
                    'fallback_cb':'wp_bootstrap_navwalker::fallback'
                }) }}


            </div>
        {% endif %}
    </nav>
  </div>
</header>
